    "dirty-chai": "^2.0.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mock-cwd": "^1.0.0",
    "mssql": "^8.1.0",
    "mysql": "^2.18.1",
    "p-each-series": "^3.0.0",
    "p-event": "^5.0.1",
    "pg": "^8.7.3",
    "sqlite3": "^5.0.2"
  },
  "peerDependencies": {
    "mssql": "^7.0.0 || ^8.0.0",
    "mysql": "^2.0.0",
    "sqlite3": "^5.0.0",
    "pg": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "mssql": {
      "optional": true
    },
    "mysql": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
